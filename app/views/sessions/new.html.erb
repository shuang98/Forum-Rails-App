<h1>Login</h1>
<% if flash[:error] %>
    <div class="alert alert-danger"><%= flash[:error] %></div>
<% end %>
<%= form_tag '/login' do %>
	
  	Email: <%= text_field_tag :email,"", class: "form-control"%> <br>
  	Password: <%= password_field_tag :password, "", class: "form-control" %> <br>
  	<%= submit_tag "Submit", class: "btn btn-primary" %>

<% end %>
